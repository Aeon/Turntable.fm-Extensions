{
  "name": "Turntable.fm Extended",
  "version": "0.1",
  "description": "Adds Desktop Notifications to turntable.fm events",
  "content_scripts": [
    {
      "matches": ["http://turntable.fm/*"],
      "css": ["css/turntable.fm.extend.css"],
      "js": ["js/jquery.1.6.1.min.js", "js/injectScript.js"]
    }
  ],
  "permissions": [
	"tabs",
	"notifications",
	"http://turntable.fm/*"
  ]
}